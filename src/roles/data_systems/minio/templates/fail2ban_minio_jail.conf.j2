# Fail2ban jail for MinIO
# Generated by Ansible - do not edit manually

[minio]
enabled = true
port = {{ minio_server_port }},{{ minio_console_port }}
filter = minio
logpath = {{ minio_audit_log_path if minio_enable_audit_logging else '/var/log/syslog' }}
maxretry = 3
bantime = 3600
findtime = 600
