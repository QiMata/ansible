# RabbitMQ configuration generated by Ansible
listeners.tcp.default = {{ rabbitmq_port }}
management.tcp.port = {{ rabbitmq_management_port }}
prometheus.tcp.port = {{ rabbitmq_prometheus_port }}
loopback_users.guest = false
cluster_partition_handling = {{ rabbitmq_cluster_partition_handling }}
{% if rabbitmq_clustered %}
cluster_formation.peer_discovery_backend = classic_config
cluster_formation.classic_config.nodes.1 = rabbit@{{ rabbitmq_cluster_master_node }}
{% endif %}
